<?xml version="1.0"?>
<odoo>
    <template id="report_res_partner_document"> <!-- Cambia a 'report_res_partner_document' -->
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-foreach="doc" t-as="contact">
                    <div>
                        <h2><t t-esc="contact.name"/></h2>
                        <p>NIT: <t t-esc="contact.vat"/></p>
                        <p>Teléfono: <t t-esc="contact.phone"/></p>
                        <p>Dirección: <t t-esc="contact.street"/></p>
                        <p>Correo: <t t-esc="contact.email"/></p>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>